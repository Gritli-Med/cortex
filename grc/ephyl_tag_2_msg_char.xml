<?xml version="1.0"?>
<block>
  <name>Tag to Message (char)</name>
  <key>ephyl_tag_2_msg_char</key>
  <category>[ephyl]</category>
  <import>import ephyl</import>
  <make>ephyl.tag_2_msg_char($wanted_tag)</make>

  <param>
    <name>Trigger Tag</name>
    <key>wanted_tag</key>
    <value>"corr_start"</value>
    <type>string</type>
  </param>

  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>

  <source>
    <name>slot_msg</name>
    <type>message</type>
  </source>

</block>